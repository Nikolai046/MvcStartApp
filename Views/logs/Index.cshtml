@{
    ViewData["Title"] = "Отчет всех запросов к приложению";
}

<style>
    table {
        border-spacing: 0 15px;
        align-self: center;
    }

    th {
        background-color: #4287f5;
        color: white;
    }

    th,
    td {
        text-align: left;
        border: 1px solid black;
        padding: 5px;
    }

    h2 {
        color: #4287f5;
    }
</style>

<div class="text-center">
    <h2 class="display-4" style="text-align: left">Пользователи проекта</h2>
    <br>
    <table>
        <tr>
            <td>Время</td>
            <td>Адрес</td>
        </tr>

        @{
            int n = 0;
            foreach (var request in Model)
            {
                n++;
                <tr>
                    <td>@request.Date.ToString("yyyy-MM-dd HH:mm:ss.fffff")</td>
                    <td>@request.Url</td>
                </tr>
            }
        }
    </table>
</div>